#Question 2:

#!/bin/bash
while read p;
do 
	if [ ! -z "$p" ]
   then
	cp $p $( echo $p | sed "s/$p/..\\/..\\/new\\/ACST890_s1_2018_$p.txt/")

fi

done < 'current_list'
#"done <'current_list'" appoints where to read values for p
